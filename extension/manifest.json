{
    "manifest_version": 2,
    "name": "Попапы с сообщениями",
    "description": "Ааааааааааааааааааааааааааааааааааааааааааааааааааа",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*.google.com/*"
            ],
            "js": [
                "main.js"
            ],
            "css": [
                "main.css"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "storage"
    ]
}